{
    "manifest_version": 3,
    
    "name": "Grandstream Calling",
    "description": "use http protocol to generate calls from browser",
    "version": "1.0.13",
    "options_ui": {
        "page": "options.html"
    },
    "icons": {
        "16": "favicon.png",
        "48": "favicon.png",
        "128": "favicon.png"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "all_frames": true,
            "js": ["background.js","tel_finder.js"],
            "run_at": "document_end"
        }
    ],
    "permissions": [
        "notifications",
        "storage",
        "activeTab",
        "storage",
        "contextMenus"
    ],"host_permissions": [
        "*://*/*"
      ],
    "action": {
        "default_icon": "favicon.png",
        "default_popup": "popup.html"
    },
"background": {
    "service_worker": "start.js"
  }
}
